{
  "name": "@deskpro/token-field",
  "version": "0.0.3",
  "description": "",
  "main": "index.js",
  "private": false,
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "npm run clean && webpack --env build",
    "lint": "eslint . --cache --cache-location=.cache/eslint --ext .js,.jsx",
    "precommit": "lint-staged",
    "prepublish": "npm run compile",
    "storybook:dev": "start-storybook -p 9003 -c .storybook",
    "test": "node scripts/test.js --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deskpro/token-field.git"
  },
  "author": "DeskPRO <team@deskpro.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/deskpro/token-field/issues"
  },
  "homepage": "https://github.com/deskpro/token-field#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "deskpro-components": "^0.5.19",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-onclickoutside": "^6.5.0"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.2.9",
    "@storybook/react": "^3.2.8",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "husky": "^0.14.3",
    "jest": "^21.1.0",
    "lint-staged": "^4.2.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "postcss-modules-values": "^1.3.0",
    "sass-loader": "^6.0.6",
    "webpack": "^3.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/tests/jest/**/?(*).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ]
  }
}
